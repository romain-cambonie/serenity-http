{
  "name": "@serenity-dev/http-client",
  "version": "0.0.18",
  "description": "WIP: An httpClient with additional functionalities",
  "keywords": [
    "httpclient",
    "axios"
  ],
  "homepage": "https://npmjs.com/package/@serenity-dev/http-client",
  "scripts": {
    "build": "yarn build-esm && yarn build-cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "eslint --config=./.tooling/.eslint/.eslintrc.cjs  ./src/",
    "lint-staged": "lint-staged --config=./.tooling/.lintstaged/.lintstagedrc",
    "prettier": "prettier --config=./.tooling/.prettier/.prettierrc.json  --write  ./src/",
    "prettier:check": "prettier --config=./.tooling/.prettier/.prettierrc.json --check ./src/",
    "test": "jest --config=./.tooling/.jest/jest.config.ts"
  },
  "author": "",
  "license": "ISC",
  "contributors": [],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/romain-cambonie/serenity-http-client.git"
  },
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "dependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.7",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
